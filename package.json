{
    "name": "@diablohu/react-router-v3",
    "version": "3.2.7",
    "description": "A complete routing library for React",
    "files": [
        "*.md",
        "docs",
        "es",
        "lib",
        "umd"
    ],
    "main": "lib/index",
    "module": "es/index",
    "jsnext:main": "es/index",
    "repository": "https://github.com/cmux/react-router-v3/tree/v3",
    "homepage": "https://github.com/cmux/react-router-v3/tree/v3#readme",
    "bugs": "https://github.com/cmux/react-router-v3/tree/v3/issues",
    "scripts": {
        "build": "npm run build-cjs && npm run build-es",
        "build-cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel ./modules -d lib --ignore __tests__",
        "build-es": "rimraf es && cross-env BABEL_ENV=es babel ./modules -d es --ignore __tests__",
        "build-umd": "cross-env NODE_ENV=development webpack modules/index.js umd/ReactRouter.js",
        "build-min": "cross-env NODE_ENV=production webpack -p modules/index.js umd/ReactRouter.min.js",
        "lint": "eslint examples modules scripts tools *.js",
        "prepublishOnly": "node ./scripts/build.js",
        "start": "node examples/server.js",
        "test": "npm run lint && npm run test-node && npm run test-browser",
        "test-browser": "cross-env NODE_ENV=test karma start",
        "test-node": "cross-env NODE_ENV=test mocha --require babel-register tests.node.js",
        "commit": "node ./node_modules/cz-customizable/standalone.js",
        "prepare": "husky install",
        "up": "yarn upgrade-interactive --latest"
    },
    "authors": [
        "Ryan Florence",
        "Michael Jackson"
    ],
    "license": "MIT",
    "dependencies": {
        "create-react-class": "^15.5.1",
        "history": "^3.0.0",
        "hoist-non-react-statics": "^3.3.2",
        "invariant": "^2.2.1",
        "loose-envify": "^1.2.0",
        "prop-types": "^15.7.2",
        "react-is": "^16.13.0",
        "warning": "^3.0.0"
    },
    "peerDependencies": {
        "react": "^0.14.0 || ^15.0.0 || ^16.0.0 || ^17.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^12.1.1",
        "@commitlint/config-conventional": "^12.1.1",
        "babel-cli": "^6.11.4",
        "babel-core": "^6.13.2",
        "babel-eslint": "^7.2.1",
        "babel-loader": "^6.2.4",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-dev-expression": "^0.2.1",
        "babel-plugin-istanbul": "^4.1.1",
        "babel-polyfill": "^6.13.0",
        "babel-preset-es2015": "^6.13.2",
        "babel-preset-react": "^6.11.1",
        "babel-preset-stage-1": "^6.13.0",
        "babel-register": "^6.11.6",
        "bundle-loader": "^0.5.4",
        "codecov": "^3.0.0",
        "cross-env": "^5.1.3",
        "css-loader": "^0.27.3",
        "cz-customizable": "^6.3.0",
        "eslint": "^7.27.0",
        "eslint-config-koot": "^3.1.1",
        "eslint-config-rackt": "^1.1.1",
        "eslint-plugin-react": "^6.2.2",
        "expect": "^1.20.2",
        "express": "^4.14.0",
        "express-urlrewrite": "^1.2.0",
        "gzip-size": "^4.1.0",
        "husky": "^6.0.0",
        "karma": "^4.1.0",
        "karma-browserstack-launcher": "^1.0.1",
        "karma-chrome-launcher": "^2.0.0",
        "karma-coverage": "^1.1.1",
        "karma-mocha": "^1.1.1",
        "karma-mocha-reporter": "^2.0.5",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.3",
        "lint-staged": "^11.0.0",
        "mocha": "^5.0.4",
        "prettier": "^2.3.0",
        "prettier-eslint": "^12.0.0",
        "pretty-bytes": "^4.0.2",
        "qs": "^6.2.1",
        "react": "^17.0.2",
        "react-addons-css-transition-group": "^15.6.0",
        "react-dom": "^16.9.0",
        "rimraf": "^2.5.4",
        "style-loader": "^0.16.1",
        "webpack": "^1.13.1",
        "webpack-dev-middleware": "^1.6.1"
    },
    "browserify": {
        "transform": [
            "loose-envify"
        ]
    },
    "tags": [
        "react",
        "router"
    ],
    "keywords": [
        "react",
        "react-component",
        "routing",
        "route",
        "routes",
        "router"
    ],
    "publishConfig": {
        "access": "public"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,cjs,mjs,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,css,less,sass,scss}": [
            "prettier --write"
        ]
    }
}
